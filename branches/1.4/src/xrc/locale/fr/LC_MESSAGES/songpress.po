#: src\xrc\songpress.pos:1
msgid "Songpress"
msgstr "Songpress"

#: src\xrc\songpress.pos:2
msgid "MyMenuBar"
msgstr "MaBarreDeMenu"

#: src\xrc\songpress.pos:3
msgid "&File"
msgstr "&Fichier"

#: src\xrc\songpress.pos:4
msgid "&New\tCTRL+N"
msgstr "&Nouveau\tCTRL+N"

#: src\xrc\songpress.pos:5
msgid "Create a new song"
msgstr "Créer un nouveau chant"

#: src\xrc\songpress.pos:6
msgid "&Open...\tCTRL+O"
msgstr "&Ouvrir...\tCTRL+O"

#: src\xrc\songpress.pos:7
msgid "Open an existing song"
msgstr "Ouvrir un fichier existant"

#: src\xrc\songpress.pos:8
msgid "&Save\tSHIFT+F12"
msgstr "&Enregistrer\tSHIFT+F12"

