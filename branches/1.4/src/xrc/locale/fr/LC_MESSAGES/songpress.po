msgid "Export image as a raster picture"
msgstr "Exporter l'image comme une image raster"

#: src\xrc\songpress.pos:19
msgid "Export as &HTML..."
msgstr "Exporter comme &HTML..."

#: src\xrc\songpress.pos:20
msgid "Export formatted song as web page"
msgstr "Exporter le chant formaté comme une page web"

#: src\xrc\songpress.pos:21
msgid "&Exit"
msgstr "&Quitter"

#: src\xrc\songpress.pos:22
msgid "Quit Songpress"
msgstr "Quitter Songpress"

#: src\xrc\songpress.pos:23
msgid "&Edit"
msgstr "&Editer"

#: src\xrc\songpress.pos:24
msgid "&Undo\tCTRL+Z"
msgstr "&Annuler\tCTRL+Z"

#: src\xrc\songpress.pos:25
msgid "Undo last edit"
msgstr "Annuler dernière édition"

