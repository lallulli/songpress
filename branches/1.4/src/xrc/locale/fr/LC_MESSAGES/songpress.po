msgid "Move chord &left\tSHIFT+ALT+LEFT"
msgstr "Déplacer l'accord à &gauche\tSHIFT+ALT+LEFT"

#: src\xrc\songpress.pos:53
msgid "Move chord under cursor one position leftwards"
msgstr "Déplacer l'accord sous le curseur d'une position vers la gauche"

#: src\xrc\songpress.pos:54
msgid "Remove c&hords"
msgstr "Supprimer les &accords"

#: src\xrc\songpress.pos:55
msgid "Remove all chords in current selection"
msgstr "Supprimer tous les accords de la sélection courante"

#: src\xrc\songpress.pos:56
msgid "Inte&grate chords"
msgstr "In&sérer les accords"

#: src\xrc\songpress.pos:57
msgid "Integrate chords of current line with text of following line"
msgstr "Insérer les accords de la ligne courante au texte de la ligne suivante"

#: src\xrc\songpress.pos:58
msgid "&View"
msgstr "&Visualiser"

#: src\xrc\songpress.pos:59
msgid "&Toolbars"
msgstr "&Barres d'outils"

#: src\xrc\songpress.pos:60
msgid "&Standard"
msgstr "&Standard"

#: src\xrc\songpress.pos:61
msgid "Show/hide toolbar with file and common buttons"
msgstr "Afficher/Masquer la barre d'outils fichier et les boutons communs"

#: src\xrc\songpress.pos:62
msgid "&Format"
msgstr "&Formater"

#: src\xrc\songpress.pos:63
msgid "Show/hide toolbar with format tools"
msgstr "Afficher/Masquer la barre d'outils de formatage"

#: src\xrc\songpress.pos:64
msgid "&Preview"
msgstr "&Prévisualiser"

#: src\xrc\songpress.pos:65
msgid "Show/hide preview pane"
msgstr "Afficher/Masquer le panneau de prévisualisation"

